doctype
html(lang='es')
	head
		meta(charset='utf-8')
		title Ejercicio 5
		style.
			.amarillo{
				background-color: yellow;
			}
			.azul {
				background-color: blue;
			}
			.rojo {
				background-color: red;
			}
	body
		h1 Ejercicio 5
		hr
		select
			-for ( var i = 1; i<= 30; i++)
				if (i%3 == 0)
					option.amarillo Elemento #{i}
				if (i%4 == 0)
					option.azul Elemento #{i}
				if (i%5 == 0)
					option.rojo Elemento #{i}
				